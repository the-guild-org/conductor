{
  "name": "website",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.11.0"
  },
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build && next-sitemap --config next-sitemap.config.cjs && next export",
    "dev": "next",
    "prettier": "pnpm prettier:check --write",
    "prettier:check": "prettier --cache --check .",
    "start": "next start"
  },
  "dependencies": {
    "@monaco-editor/react": "4.6.0",
    "@theguild/components": "6.1.0",
    "@theguild/prettier-config": "^2.0.2",
    "clsx": "2.0.0",
    "graphql": "16.8.1",
    "json-to-pretty-yaml": "1.2.2",
    "next": "13.5.6",
    "next-sitemap": "4.2.3",
    "next-themes": "0.2.1",
    "nextra": "3.0.0-alpha.10",
    "nextra-theme-docs": "3.0.0-alpha.10",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "0.5.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0"
  },
  "devDependencies": {
    "@theguild/tailwind-config": "0.3.1",
    "@types/json-schema": "7.0.15",
    "@types/json-to-pretty-yaml": "1.2.1",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "typescript": "5.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "json-to-pretty-yaml@1.2.2": "patches/json-to-pretty-yaml@1.2.2.patch"
    }
  }
}
